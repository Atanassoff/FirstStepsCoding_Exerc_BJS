function demo(input){
    let nylon = (Number(input[0]) + 2) * 1.5; 
    let paint = (Number(input[1]) * 1.1) * 14.5;
    let liquid = Number(input[2]) * 5;
    let hours = Number(input[3]);
    let materials = nylon + paint + liquid + 0.4;
    let workers = materials * 0.3 * hours;
    let total = materials + workers;
    console.log(total);
}
